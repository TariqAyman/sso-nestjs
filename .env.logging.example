# Logging Configuration Options

# Set the log level (NONE, ERROR, WARN, INFO, DEBUG, VERBOSE)
LOG_LEVEL=DEBUG

# Enable/disable request logging (true/false)
ENABLE_REQUEST_LOGGING=true

# Enable/disable detailed request/response logging (true/false)
ENABLE_DETAILED_LOGGING=true

# Log sensitive data like passwords, tokens, etc. (true/false)
# WARNING: Never set this to true in production!
LOG_SENSITIVE_DATA=false
